$('#photos').html("<%= j render 'photos_list' %>")
<% if @gym.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@gym.active && !@gym.price.blank? && !@gym.listing_name.blank? && !@gym.photos.blank? && !@gym.address.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>